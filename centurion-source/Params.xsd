<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="params">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="camera">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="xPosition" type="xs:integer" />
              <xs:element name="yPosition" type="xs:integer" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="mouse">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="scrollValue" type="xs:integer" />
              <xs:element name="xLeftClick" type="xs:integer" />
              <xs:element name="xPosition" type="xs:integer" />
              <xs:element name="xRightClick" type="xs:integer" />
              <xs:element name="y2dPosition" type="xs:integer" />
              <xs:element name="y2dRightClick" type="xs:integer" />
              <xs:element name="yLeftClick" type="xs:integer" />
              <xs:element name="yPosition" type="xs:integer" />
              <xs:element name="yRightClick" type="xs:integer" />
              <xs:element name="leftClick" type="xs:boolean" />
              <xs:element name="leftHold" type="xs:boolean" />
              <xs:element name="release" type="xs:boolean" />
              <xs:element name="rightClick" type="xs:boolean" />
              <xs:element name="scrollBool" type="xs:boolean" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="window">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="heightZoomed" type="xs:decimal" />
              <xs:element name="ratio" type="xs:decimal" />
              <xs:element name="widthZoomed" type="xs:decimal" />
              <xs:element name="shouldClose" type="xs:boolean" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ui">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="bottomBarHeight" type="xs:integer" />
              <xs:element name="topBarHeight" type="xs:integer" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>