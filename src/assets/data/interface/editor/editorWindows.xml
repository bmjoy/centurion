  <!-- OBJECT PROPERTIES WINDOW -->

  <editorWindow id="3">
    <iframe id="3" type="editor" title="EDITOR_properties" position="x=Engine.WindowWidth/2-w/2; y=Engine.WindowHeight/2-h/2;" size="w=600; h=450;" isOpened="false">
		<conditionScript function="ConditionProperties">
		  -- lua script
		  function ConditionProperties()
		  condition1 = Mouse.RightClick
		  condition2 = Game.IsObjectNotNull(Mouse.GetRightClickId())
		  condition3 = Game.IsObjectSelected(Mouse.GetRightClickId())
		  return condition1 and condition2 and condition3
		  end
		</conditionScript>
		<openingScript>
		  -- lua script
		  Iframe.GetIframeById(3):UpdateStringBySimpleTextId(1, Selo():GetClassName())
		  Iframe.GetIframeById(3):UpdateTextInputPlaceholder(0, Selo():GetIDName())
		  Iframe.GetIframeById(3):UpdateTextInputPlaceholder(1, Selo():GetDisplayedName())
		</openingScript>
		<simpleTextArray>
		  <simpleText id="0" name="EDITOR_propertiesClassName" xOffset="20" yOffset="-80" />
		  <simpleText id="1" name="" xOffset="150" yOffset="-80" />
		  <simpleText id="2" name="EDITOR_propertiesIDName" xOffset="20" yOffset="-110" />
		  <simpleText id="3" name="EDITOR_propertiesDisplayedName" xOffset="20" yOffset="-140" />
		</simpleTextArray>
		<buttonArray>
			<button text="WORD_Close" xOffset="400" yOffset="30">
			<onclickScript>
			  -- lua script
			  Editor.CloseWindow(3)
			</onclickScript>
		  </button>
		  <button text="WORD_Save" xOffset="520" yOffset="30">
			<onclickScript>
			  -- lua script
			  Selo():SetIDName(Iframe.GetIframeById(3):GetTextInputById(0):GetText())
			  Selo():SetDisplayedName(Iframe.GetIframeById(3):GetTextInputById(1):GetText())
			  Editor.CloseWindow(3)
			</onclickScript>
		  </button>
		</buttonArray>
		<textListArray></textListArray>
		<textInputArray>
		  <textInput textInputId="0" xOffset="150" yOffset="-110" width="200" placeholder=""/>
		  <textInput textInputId="1" xOffset="150" yOffset="-140" width="200" placeholder=""/>
		</textInputArray>
	</iframe>
  </editorWindow>


