    <conditionScript function="ConditionProperties">
      -- lua script
      function ConditionProperties()
      condition1 = Mouse.RightClick
      condition2 = Game.IsObjectNotNull(Mouse.RightClickID)
      condition3 = Game.IsObjectSelected(Mouse.RightClickID)
      return condition1 and condition2 and condition3
