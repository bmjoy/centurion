  <!-- OBJECT PROPERTIES WINDOW -->

  <editorWindow id="4" closeOtherWindowsWhenOpen="true">
    <iframe id="4" type="editor" title="EDITOR_properties" position="x=Engine.WindowWidth/2-w/2; y=Engine.WindowHeight/2-h/2;" size="w=600; h=450;" isOpened="false">
      <conditionScript function="ConditionProperties">
        -- lua script
        function ConditionProperties()
        condition1 = Mouse.RightClick
        condition2 = Game.IsObjectNotNull(Mouse.GetRightClickId())
        condition3 = Game.IsObjectSelected(Mouse.GetRightClickId())
        return condition1 and condition2 and condition3
        end
      </conditionScript>
      <openingScript>
        -- lua script
        thisIframe = Iframe.GetIframeById(4)
        thisIframe:UpdateStringBySimpleTextId(1, Selo():GetClassName())
        thisIframe:UpdateTextInputPlaceholder(0, Selo():GetIDName())
        thisIframe:UpdateTextInputPlaceholder(1, Selo():GetDisplayedName())
        thisIframe:UpdateTextInputPlaceholder(2, Selo():AsBuilding():GetSettlement():GetSettlementName())
      </openingScript>
      <simpleTextArray>
        <simpleText id="0" name="EDITOR_propertiesClassName" xOffset="20" yOffset="-80" />
        <simpleText id="1" name="" xOffset="150" yOffset="-80" />
        <simpleText id="2" name="EDITOR_propertiesIDName" xOffset="20" yOffset="-110" />
        <simpleText id="3" name="EDITOR_propertiesDisplayedName" xOffset="20" yOffset="-140" />
        <simpleText id="4" name="EDITOR_propertiesSettlement" xOffset="20" yOffset="-170" />
      </simpleTextArray>
      <textBoxArray></textBoxArray>
      <buttonArray>
        <button text="WORD_Close" xOffset="400" yOffset="30">
          <onclickScript>
            -- lua script
            Editor.CloseWindow(4)
          </onclickScript>
        </button>
        <button text="WORD_Save" xOffset="520" yOffset="30">
          <onclickScript>
            -- lua script
            Selo():SetIDName(Iframe.GetIframeById(4):GetTextInputById(0):GetText())
            Selo():SetDisplayedName(Iframe.GetIframeById(4):GetTextInputById(1):GetText())
            Selo():AsBuilding():GetSettlement():SetSettlementName(Iframe.GetIframeById(4):GetTextInputById(2):GetText())
            Editor.MarkAsEdited()
            Editor.CloseWindow(4)
          </onclickScript>
        </button>
      </buttonArray>
      <textListArray></textListArray>
      <textInputArray>
        <textInput textInputId="0" xOffset="180" yOffset="-110" width="200" placeholder=""/>
        <textInput textInputId="1" xOffset="180" yOffset="-140" width="200" placeholder=""/>
        <textInput textInputId="2" xOffset="180" yOffset="-170" width="200" placeholder=""/>
      </textInputArray>
    </iframe>
  </editorWindow>


  <!-- ADD OBJECT WINDOW -->

  <editorWindow id="5" closeOtherWindowsWhenOpen="true">
    <iframe id="5" type="editor" title="EDITOR_addObject" position="x=Engine.WindowWidth/2-w/2; y=0" size="w=600; h=250;" isOpened="false">
      <conditionScript function="ConditionAddObject">
        -- lua script
        function ConditionAddObject()
        condition1 = Keyboard.IsKeyPressed(Keyboard.KEY_LEFT_CONTROL) or Keyboard.IsKeyPressed(Keyboard.KEY_RIGHT_CONTROL)
        condition2 = Keyboard.IsKeyPressed(Keyboard.KEY_A)
        return condition1 and condition2
        end
      </conditionScript>
      <openingScript>
        -- lua script
        Game.ResetSelectedObject()
        _editorList1 = Editor.GetObjectTreeList1()
        Iframe.GetIframeById(5):UpdateTextListById(0, _editorList1, "EDITORTREE_")
        _editorList2 = Editor.GetObjectTreeList2(Iframe.GetIframeById(5):GetTextListById(0):GetSelectedOption())
        Iframe.GetIframeById(5):UpdateTextListById(1, _editorList2, "EDITORTREE_")
        _editorList3 = Editor.GetObjectTreeList3(Iframe.GetIframeById(5):GetTextListById(0):GetSelectedOption(), Iframe.GetIframeById(5):GetTextListById(1):GetSelectedOption())
        Iframe.GetIframeById(5):UpdateTextListById(2, _editorList3, "WORD_")
      </openingScript>
      <simpleTextArray></simpleTextArray>
      <textBoxArray></textBoxArray>
      <buttonArray>
        <button text="WORD_Close" xOffset="400" yOffset="30">
          <onclickScript>
            --lua script
            Editor.CloseWindow(5)
          </onclickScript>
        </button>
        <button text="WORD_Add" xOffset="520" yOffset="30">
          <onclickScript>
            Editor.InsertObject(Iframe.GetIframeById(5):GetTextListById(0):GetSelectedOption(), Iframe.GetIframeById(5):GetTextListById(2):GetSelectedOption())
          </onclickScript>
        </button>
      </buttonArray>
      <textListArray>
        <textList textListId="0" xOffset="20" yOffset="185" maxOptions="5" width="163">
          <onclickScript>
            _editorList2 = Editor.GetObjectTreeList2(Iframe.GetIframeById(5):GetTextListById(0):GetSelectedOption())
            Iframe.GetIframeById(5):UpdateTextListById(1, _editorList2, "EDITORTREE_")
            _editorList3 = Editor.GetObjectTreeList3(Iframe.GetIframeById(5):GetTextListById(0):GetSelectedOption(), Iframe.GetIframeById(5):GetTextListById(1):GetSelectedOption())
            Iframe.GetIframeById(5):UpdateTextListById(2, _editorList3, "WORD_")
          </onclickScript>
        </textList>
        <textList textListId="1" xOffset="220" yOffset="185" maxOptions="5" width="163">
          <onclickScript>
            _editorList3 = Editor.GetObjectTreeList3(Iframe.GetIframeById(5):GetTextListById(0):GetSelectedOption(), Iframe.GetIframeById(5):GetTextListById(1):GetSelectedOption())
            Iframe.GetIframeById(5):UpdateTextListById(2, _editorList3, "WORD_")
          </onclickScript>
        </textList>
        <textList textListId="2" xOffset="420" yOffset="185" maxOptions="5" width="163">
        </textList>
      </textListArray>
      <textInputArray></textInputArray>
    </iframe>
