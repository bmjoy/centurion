  <!-- OBJECT PROPERTIES WINDOW -->

  <editorWindow id="3" iframe_id="3" name="EDITOR_properties" iframe="editor" position="x=Engine.WindowWidth/2-w/2; y=Engine.WindowHeight/2-h/2;" size="w=600; h=450;" isOpened="false">
    <conditionScript function="ConditionProperties">
      -- lua script
      function ConditionProperties()
      condition1 = Mouse.RightClick
      condition2 = Game.IsObjectNotNull(Mouse.GetRightClickId())
      condition3 = Game.IsObjectSelected(Mouse.GetRightClickId())
      return condition1 and condition2 and condition3
      end
    </conditionScript>
    <openingScript>
      -- lua script
      Iframe.GetIframeById(3):UpdateStringBySimpleTextId(1, Selo():GetClassName())
      Iframe.GetIframeById(3):UpdateTextInputPlaceholder(0, "ciao")
    </openingScript>
    <simpleTextArray>
      <simpleText id="0" name="Class name: " xOffset="20" yOffset="-80" />
